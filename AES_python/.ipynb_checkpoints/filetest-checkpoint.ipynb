    "def linecount_1():\n",
    "    return len(open('AES.py').readlines())\n",
    "\n",
    "def countStr(str):\n",
    "    thefile = open('AES.py')\n",
    "    count = 0;\n",
    "    buffer = thefile.read()\n",
    "    count += buffer.count(str)\n",
    "\n",
    "    return count\n",
    "\n",
